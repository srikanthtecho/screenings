#!/usr/bin/env sh

##############################################################################
##
##  gateway start up script for UN*X
##
##############################################################################

# Attempt to set APP_HOME
# Resolve links: $0 may be a link
PRG="$0"
# Need this for relative symlinks.
while [ -h "$PRG" ] ; do
    ls=`ls -ld "$PRG"`
    link=`expr "$ls" : '.*-> \(.*\)$'`
    if expr "$link" : '/.*' > /dev/null; then
        PRG="$link"
    else
        PRG=`dirname "$PRG"`"/$link"
    fi
done
SAVED="`pwd`"
cd "`dirname \"$PRG\"`/.." >/dev/null
APP_HOME="`pwd -P`"
cd "$SAVED" >/dev/null

APP_NAME="gateway"
APP_BASE_NAME=`basename "$0"`

# Add default JVM options here. You can also use JAVA_OPTS and GATEWAY_OPTS to pass JVM options to this script.
DEFAULT_JVM_OPTS=""

# Use the maximum available, or set MAX_FD != -1 to use that value.
MAX_FD="maximum"

warn ( ) {
    echo "$*"
}

die ( ) {
    echo
    echo "$*"
    echo
    exit 1
}

# OS specific support (must be 'true' or 'false').
cygwin=false
msys=false
darwin=false
nonstop=false
case "`uname`" in
  CYGWIN* )
    cygwin=true
    ;;
  Darwin* )
    darwin=true
    ;;
  MINGW* )
    msys=true
    ;;
  NONSTOP* )
    nonstop=true
    ;;
esac

CLASSPATH=$APP_HOME/lib/gateway.jar:$APP_HOME/lib/spring-boot-starter-web-1.5.2.RELEASE.jar:$APP_HOME/lib/spring-boot-devtools-1.5.2.RELEASE.jar:$APP_HOME/lib/spring-boot-starter-actuator-1.5.2.RELEASE.jar:$APP_HOME/lib/spring-boot-starter-freemarker-1.5.2.RELEASE.jar:$APP_HOME/lib/spring-cloud-starter-eureka-1.3.1.RELEASE.jar:$APP_HOME/lib/spring-cloud-starter-zuul-1.3.1.RELEASE.jar:$APP_HOME/lib/bootstrap-3.3.7-1.jar:$APP_HOME/lib/jquery-3.1.1-1.jar:$APP_HOME/lib/h2-1.3.156.jar:$APP_HOME/lib/interview.jar:$APP_HOME/lib/position.jar:$APP_HOME/lib/skill.jar:$APP_HOME/lib/spring-boot-starter-1.5.2.RELEASE.jar:$APP_HOME/lib/spring-boot-starter-tomcat-1.5.2.RELEASE.jar:$APP_HOME/lib/hibernate-validator-5.3.4.Final.jar:$APP_HOME/lib/jackson-databind-2.8.7.jar:$APP_HOME/lib/spring-web-4.3.7.RELEASE.jar:$APP_HOME/lib/spring-webmvc-4.3.7.RELEASE.jar:$APP_HOME/lib/spring-boot-1.5.2.RELEASE.jar:$APP_HOME/lib/spring-boot-autoconfigure-1.5.2.RELEASE.jar:$APP_HOME/lib/spring-boot-actuator-1.5.2.RELEASE.jar:$APP_HOME/lib/freemarker-2.3.25-incubating.jar:$APP_HOME/lib/spring-context-support-4.3.7.RELEASE.jar:$APP_HOME/lib/spring-cloud-starter-1.2.2.RELEASE.jar:$APP_HOME/lib/spring-cloud-netflix-core-1.3.1.RELEASE.jar:$APP_HOME/lib/spring-cloud-netflix-eureka-client-1.3.1.RELEASE.jar:$APP_HOME/lib/eureka-client-1.6.2.jar:$APP_HOME/lib/eureka-core-1.6.2.jar:$APP_HOME/lib/spring-cloud-starter-archaius-1.3.1.RELEASE.jar:$APP_HOME/lib/spring-cloud-starter-ribbon-1.3.1.RELEASE.jar:$APP_HOME/lib/ribbon-eureka-2.2.2.jar:$APP_HOME/lib/xstream-1.4.9.jar:$APP_HOME/lib/spring-cloud-starter-hystrix-1.3.1.RELEASE.jar:$APP_HOME/lib/zuul-core-1.3.0.jar:$APP_HOME/lib/spring-boot-starter-data-jpa-1.5.2.RELEASE.jar:$APP_HOME/lib/spring-boot-starter-logging-1.5.2.RELEASE.jar:$APP_HOME/lib/spring-core-4.3.7.RELEASE.jar:$APP_HOME/lib/snakeyaml-1.17.jar:$APP_HOME/lib/tomcat-embed-core-8.5.11.jar:$APP_HOME/lib/tomcat-embed-el-8.5.11.jar:$APP_HOME/lib/tomcat-embed-websocket-8.5.11.jar:$APP_HOME/lib/validation-api-1.1.0.Final.jar:$APP_HOME/lib/jboss-logging-3.3.0.Final.jar:$APP_HOME/lib/classmate-1.3.3.jar:$APP_HOME/lib/jackson-annotations-2.8.0.jar:$APP_HOME/lib/jackson-core-2.8.7.jar:$APP_HOME/lib/spring-aop-4.3.7.RELEASE.jar:$APP_HOME/lib/spring-beans-4.3.7.RELEASE.jar:$APP_HOME/lib/spring-context-4.3.7.RELEASE.jar:$APP_HOME/lib/spring-expression-4.3.7.RELEASE.jar:$APP_HOME/lib/spring-cloud-context-1.2.2.RELEASE.jar:$APP_HOME/lib/spring-cloud-commons-1.2.2.RELEASE.jar:$APP_HOME/lib/spring-security-rsa-1.0.3.RELEASE.jar:$APP_HOME/lib/jettison-1.3.7.jar:$APP_HOME/lib/netflix-eventbus-0.3.0.jar:$APP_HOME/lib/archaius-core-0.7.4.jar:$APP_HOME/lib/jsr311-api-1.1.1.jar:$APP_HOME/lib/servo-core-0.10.1.jar:$APP_HOME/lib/jersey-core-1.19.1.jar:$APP_HOME/lib/jersey-client-1.19.1.jar:$APP_HOME/lib/jersey-apache-client4-1.19.1.jar:$APP_HOME/lib/httpclient-4.5.3.jar:$APP_HOME/lib/guice-4.1.0.jar:$APP_HOME/lib/woodstox-core-asl-4.4.1.jar:$APP_HOME/lib/commons-configuration-1.8.jar:$APP_HOME/lib/guava-18.0.jar:$APP_HOME/lib/ribbon-2.2.2.jar:$APP_HOME/lib/ribbon-core-2.2.2.jar:$APP_HOME/lib/ribbon-httpclient-2.2.2.jar:$APP_HOME/lib/ribbon-loadbalancer-2.2.2.jar:$APP_HOME/lib/rxjava-1.1.10.jar:$APP_HOME/lib/slf4j-api-1.7.24.jar:$APP_HOME/lib/xmlpull-1.1.3.1.jar:$APP_HOME/lib/xpp3_min-1.1.4c.jar:$APP_HOME/lib/hystrix-core-1.5.12.jar:$APP_HOME/lib/hystrix-metrics-event-stream-1.5.12.jar:$APP_HOME/lib/hystrix-javanica-1.5.12.jar:$APP_HOME/lib/commons-io-2.4.jar:$APP_HOME/lib/netflix-commons-util-0.1.1.jar:$APP_HOME/lib/spring-boot-starter-aop-1.5.2.RELEASE.jar:$APP_HOME/lib/spring-boot-starter-jdbc-1.5.2.RELEASE.jar:$APP_HOME/lib/hibernate-core-5.0.12.Final.jar:$APP_HOME/lib/hibernate-entitymanager-5.0.12.Final.jar:$APP_HOME/lib/javax.transaction-api-1.2.jar:$APP_HOME/lib/spring-data-jpa-1.11.1.RELEASE.jar:$APP_HOME/lib/spring-aspects-4.3.7.RELEASE.jar:$APP_HOME/lib/logback-classic-1.1.11.jar:$APP_HOME/lib/jcl-over-slf4j-1.7.24.jar:$APP_HOME/lib/jul-to-slf4j-1.7.24.jar:$APP_HOME/lib/log4j-over-slf4j-1.7.24.jar:$APP_HOME/lib/spring-security-crypto-4.2.2.RELEASE.jar:$APP_HOME/lib/bcpkix-jdk15on-1.55.jar:$APP_HOME/lib/stax-api-1.0.1.jar:$APP_HOME/lib/netflix-infix-0.3.0.jar:$APP_HOME/lib/commons-math-2.2.jar:$APP_HOME/lib/servo-internal-0.10.1.jar:$APP_HOME/lib/httpcore-4.4.6.jar:$APP_HOME/lib/commons-codec-1.10.jar:$APP_HOME/lib/javax.inject-1.jar:$APP_HOME/lib/aopalliance-1.0.jar:$APP_HOME/lib/stax-api-1.0-2.jar:$APP_HOME/lib/stax2-api-3.1.4.jar:$APP_HOME/lib/commons-lang-2.6.jar:$APP_HOME/lib/ribbon-transport-2.2.2.jar:$APP_HOME/lib/rxnetty-0.4.9.jar:$APP_HOME/lib/commons-collections-3.2.2.jar:$APP_HOME/lib/netflix-statistics-0.1.1.jar:$APP_HOME/lib/HdrHistogram-2.1.9.jar:$APP_HOME/lib/hystrix-serialization-1.5.12.jar:$APP_HOME/lib/commons-lang3-3.1.jar:$APP_HOME/lib/asm-5.0.4.jar:$APP_HOME/lib/aspectjweaver-1.8.9.jar:$APP_HOME/lib/tomcat-jdbc-8.5.11.jar:$APP_HOME/lib/spring-jdbc-4.3.7.RELEASE.jar:$APP_HOME/lib/hibernate-jpa-2.1-api-1.0.0.Final.jar:$APP_HOME/lib/javassist-3.21.0-GA.jar:$APP_HOME/lib/antlr-2.7.7.jar:$APP_HOME/lib/jandex-2.0.0.Final.jar:$APP_HOME/lib/dom4j-1.6.1.jar:$APP_HOME/lib/hibernate-commons-annotations-5.0.1.Final.jar:$APP_HOME/lib/spring-data-commons-1.13.1.RELEASE.jar:$APP_HOME/lib/spring-orm-4.3.7.RELEASE.jar:$APP_HOME/lib/spring-tx-4.3.7.RELEASE.jar:$APP_HOME/lib/logback-core-1.1.11.jar:$APP_HOME/lib/bcprov-jdk15on-1.55.jar:$APP_HOME/lib/commons-jxpath-1.3.jar:$APP_HOME/lib/joda-time-2.9.7.jar:$APP_HOME/lib/antlr-runtime-3.4.jar:$APP_HOME/lib/gson-2.8.0.jar:$APP_HOME/lib/rxnetty-contexts-0.4.9.jar:$APP_HOME/lib/rxnetty-servo-0.4.9.jar:$APP_HOME/lib/netty-codec-http-4.0.27.Final.jar:$APP_HOME/lib/netty-transport-native-epoll-4.0.27.Final.jar:$APP_HOME/lib/jackson-module-afterburner-2.8.7.jar:$APP_HOME/lib/tomcat-juli-8.5.11.jar:$APP_HOME/lib/stringtemplate-3.2.1.jar:$APP_HOME/lib/netty-codec-4.0.27.Final.jar:$APP_HOME/lib/netty-handler-4.0.27.Final.jar:$APP_HOME/lib/netty-common-4.0.27.Final.jar:$APP_HOME/lib/netty-buffer-4.0.27.Final.jar:$APP_HOME/lib/netty-transport-4.0.27.Final.jar

# Determine the Java command to use to start the JVM.
if [ -n "$JAVA_HOME" ] ; then
    if [ -x "$JAVA_HOME/jre/sh/java" ] ; then
        # IBM's JDK on AIX uses strange locations for the executables
        JAVACMD="$JAVA_HOME/jre/sh/java"
    else
        JAVACMD="$JAVA_HOME/bin/java"
    fi
    if [ ! -x "$JAVACMD" ] ; then
        die "ERROR: JAVA_HOME is set to an invalid directory: $JAVA_HOME

Please set the JAVA_HOME variable in your environment to match the
location of your Java installation."
    fi
else
    JAVACMD="java"
    which java >/dev/null 2>&1 || die "ERROR: JAVA_HOME is not set and no 'java' command could be found in your PATH.

Please set the JAVA_HOME variable in your environment to match the
location of your Java installation."
fi

# Increase the maximum file descriptors if we can.
if [ "$cygwin" = "false" -a "$darwin" = "false" -a "$nonstop" = "false" ] ; then
    MAX_FD_LIMIT=`ulimit -H -n`
    if [ $? -eq 0 ] ; then
        if [ "$MAX_FD" = "maximum" -o "$MAX_FD" = "max" ] ; then
            MAX_FD="$MAX_FD_LIMIT"
        fi
        ulimit -n $MAX_FD
        if [ $? -ne 0 ] ; then
            warn "Could not set maximum file descriptor limit: $MAX_FD"
        fi
    else
        warn "Could not query maximum file descriptor limit: $MAX_FD_LIMIT"
    fi
fi

# For Darwin, add options to specify how the application appears in the dock
if $darwin; then
    GRADLE_OPTS="$GRADLE_OPTS \"-Xdock:name=$APP_NAME\" \"-Xdock:icon=$APP_HOME/media/gradle.icns\""
fi

# For Cygwin, switch paths to Windows format before running java
if $cygwin ; then
    APP_HOME=`cygpath --path --mixed "$APP_HOME"`
    CLASSPATH=`cygpath --path --mixed "$CLASSPATH"`
    JAVACMD=`cygpath --unix "$JAVACMD"`

    # We build the pattern for arguments to be converted via cygpath
    ROOTDIRSRAW=`find -L / -maxdepth 1 -mindepth 1 -type d 2>/dev/null`
    SEP=""
    for dir in $ROOTDIRSRAW ; do
        ROOTDIRS="$ROOTDIRS$SEP$dir"
        SEP="|"
    done
    OURCYGPATTERN="(^($ROOTDIRS))"
    # Add a user-defined pattern to the cygpath arguments
    if [ "$GRADLE_CYGPATTERN" != "" ] ; then
        OURCYGPATTERN="$OURCYGPATTERN|($GRADLE_CYGPATTERN)"
    fi
    # Now convert the arguments - kludge to limit ourselves to /bin/sh
    i=0
    for arg in "$@" ; do
        CHECK=`echo "$arg"|egrep -c "$OURCYGPATTERN" -`
        CHECK2=`echo "$arg"|egrep -c "^-"`                                 ### Determine if an option

        if [ $CHECK -ne 0 ] && [ $CHECK2 -eq 0 ] ; then                    ### Added a condition
            eval `echo args$i`=`cygpath --path --ignore --mixed "$arg"`
        else
            eval `echo args$i`="\"$arg\""
        fi
        i=$((i+1))
    done
    case $i in
        (0) set -- ;;
        (1) set -- "$args0" ;;
        (2) set -- "$args0" "$args1" ;;
        (3) set -- "$args0" "$args1" "$args2" ;;
        (4) set -- "$args0" "$args1" "$args2" "$args3" ;;
        (5) set -- "$args0" "$args1" "$args2" "$args3" "$args4" ;;
        (6) set -- "$args0" "$args1" "$args2" "$args3" "$args4" "$args5" ;;
        (7) set -- "$args0" "$args1" "$args2" "$args3" "$args4" "$args5" "$args6" ;;
        (8) set -- "$args0" "$args1" "$args2" "$args3" "$args4" "$args5" "$args6" "$args7" ;;
        (9) set -- "$args0" "$args1" "$args2" "$args3" "$args4" "$args5" "$args6" "$args7" "$args8" ;;
    esac
fi

# Escape application args
save ( ) {
    for i do printf %s\\n "$i" | sed "s/'/'\\\\''/g;1s/^/'/;\$s/\$/' \\\\/" ; done
    echo " "
}
APP_ARGS=$(save "$@")

# Collect all arguments for the java command, following the shell quoting and substitution rules
eval set -- $DEFAULT_JVM_OPTS $JAVA_OPTS $GATEWAY_OPTS -classpath "\"$CLASSPATH\"" com.techolution.GatewayApplication "$APP_ARGS"

# by default we should be in the correct project dir, but when run from Finder on Mac, the cwd is wrong
if [ "$(uname)" = "Darwin" ] && [ "$HOME" = "$PWD" ]; then
  cd "$(dirname "$0")"
fi

exec "$JAVACMD" "$@"
